pipeline {
    environment {
        registry = "fredysa/sample"
        registryCredential = '42651425'
        dockerImage = ''
        path = "/home/"
       
    }


    agent any
    stages {
        stage('Build') {
            steps {
                sh 'echo "Hello World"'
                sh '''
                    echo "Multiline shell steps works too"
                    ls -lah
                    docker version
                '''
            }
        }
    }
}